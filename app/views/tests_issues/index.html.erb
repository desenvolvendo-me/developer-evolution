<h1>Tests Issues</h1>
<table>
  <thead>
  <tr>
    <th>Issue Link</th>
    <th>Pull Request Link</th>
    <th colspan="3"></th>
  </tr>
  </thead>
  <tbody>
  <% @tests_issues.each do |issue| %>
    <tr>
      <td><%= issue.issue_link %></td>
      <td><%= issue.pull_request_link %></td>
      <td><%= link_to 'Show', [@test, @tests_battle, issue] %></td>
      <td><%= link_to 'Edit', edit_test_tests_battle_tests_issue_path(@test, @tests_battle, issue) %></td>
      <td><%= button_to 'Destroy', [@test, @tests_battle, issue], method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
  </tbody>
</table>

<br>
<%= link_to 'New Issue', new_test_tests_battle_tests_issue_path(@test, @tests_battle) %> |
<%= link_to 'Back', test_tests_battle_path(@test, @tests_battle) %>
