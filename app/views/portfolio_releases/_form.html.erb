<%= form_for([@portfolio, @portfolio_release]) do |f| %>
  <div class="field">
    <%= f.label :milestone_release_link, t('.milestone_release_link') %>
    <%= f.text_field :milestone_release_link %>
  </div>
  <div class="field">
    <%= f.label :pull_request_release_link, t('.pull_request_release_link') %>
    <%= f.text_field :pull_request_release_link %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
