<h1><%= t('test_battle.index.title') %></h1>
<table>
  <thead>
  <tr>
    <th><%= t('test_battle.index.milestone_release_link') %></th>
    <th><%= t('test_battle.index.pull_request_release_link') %></th>
    <th colspan="3"></th>
  </tr>
  </thead>
  <tbody>
  <% @tests_battles.each do |battle| %>
    <tr>
      <td><%= battle.milestone_release_link %></td>
      <td><%= battle.pull_request_release_link %></td>
      <td><%= link_to t('test_battle.index.show'), [@test, battle] %></td>
      <td><%= link_to t('test_battle.index.edit'), edit_test_tests_battle_path(@test, battle) %></td>
      <td><%= button_to t('test_battle.index.destroy'), [@test, battle], method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
  </tbody>
</table>

<p>
<%= link_to t('test_battle.index.new_battles'), new_test_tests_battle_path(@test) %>
<%= link_to t('test_battle.index.back'), tests_path %>
</p>