<h1><%= t("technical_test.index.title") %></h1>

<table>
  <thead>
  <tr>
    <th><%= t("technical_test.index.battle") %></th>
    <th><%= t("technical_test.index.project") %></th>
    <th><%= t("technical_test.index.readme") %></th>
    <th><%= t("technical_test.index.link") %></th>
    <th><%= t("technical_test.index.release_type") %></th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @technical_tests.each do |technical_test| %>
    <tr>
      <td><%= technical_test.battle %></td>
      <td><%= technical_test.project %></td>
      <td><%= technical_test.readme %></td>
      <td><%= technical_test.link %></td>
      <td><%= technical_test.release_type %></td>
      <td><%= link_to t("technical_test.index.show"), technical_test %></td>
      <td><%= link_to t("technical_test.index.edit"), edit_technical_test_path(technical_test) %></td>
      <td><%= button_to t("technical_test.index.destroy"), technical_test, method: :delete, data: { confirm: t("technical_test.index.Are you sure?") } %></td>
    </tr>
  <% end %>
  </tbody>
</table>

<br>

<%= link_to t('technical_test.index.new_technical_test'), new_technical_test_path %>
