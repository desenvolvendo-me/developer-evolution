<%= form_with(model: @technical_test) do |form| %>
  <% if @technical_test.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@technical_test.errors.count, 'error') %> prohibited this technical test from being saved:</h2>
      <ul>
        <% @technical_test.errors.full_messages.each do |message| %>
          <li><%= message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>
  <div>
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>
  <div>
    <%= form.label :project %>
    <%= form.text_field :project %>
  </div>
  <div>
    <%= form.label :readme %>
    <%= form.text_field :readme %>
  </div>
  <div>
    <%= form.label :link %>
    <%= form.text_field :link %>
  </div>
  <div>
    <%= form.label :battle %>
    <%= form.select :battle, (1..5) %>
  </div>
  <div class="field">
    <%= form.label :release_type, t('.release_type') %>
    <%= form.select :release_type, TechnicalTest::CATEGORY %>
  </div>
  <div>
    <%= form.submit 'Create Technical Test' %>
  </div>
<% end %>
