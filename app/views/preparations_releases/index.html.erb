<h2>Preparations Releases</h2>
<ul>
  <% @preparations_releases.each do |release| %>
    <li>
      <%= "Release ##{release.id}" %> |
      <%= link_to 'Show', preparation_preparations_release_path(@preparation, release) %> |
      <%= link_to 'Edit', edit_preparation_preparations_release_path(@preparation, release) %> |
      <%= button_to 'Delete', preparation_preparations_release_path(@preparation, release),
                  method: :delete, data: { confirm: 'Are you sure?' } %>
    </li>
  <% end %>
</ul>
<%= link_to 'New Preparations Release', new_preparation_preparations_release_path(@preparation) %> |
<%= link_to 'Back', preparation_path(@preparation) %>
